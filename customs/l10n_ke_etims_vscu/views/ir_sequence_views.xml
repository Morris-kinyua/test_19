<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sequence_tree_ke_etims" model="ir.ui.view">
        <field name="name">ir.sequence.tree.ke.etims</field>
        <field name="model">ir.sequence</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list string="eTIMS Sequences">
                <field name="name"/>
                <field name="code"/>
                <field name="number_next_actual"/>
                <field name="company_id" groups="base.group_multi_company"/>
            </list>
        </field>
    </record>

    <record id="action_sequence_ke_etims" model="ir.actions.act_window">
        <field name="name">eTIMS Sequences</field>
        <field name="res_model">ir.sequence</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_sequence_tree_ke_etims"/>
        <field name="domain">[('code', 'in', ['l10n.ke.oscu.sale.sequence', 'l10n.ke.oscu.purchase.sequence'])]</field>
        <field name="context">{'default_implementation': 'no_gap'}</field>
    </record>

    <menuitem id="menu_ke_etims_sequences"
              name="eTIMS Sequences"
              parent="account.menu_finance_configuration"
              action="action_sequence_ke_etims"
              sequence="50"/>
</odoo>